<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/color_f5f5f5"
    tools:context=".ui.activity.MyOutDetailActivity"
    android:fitsSystemWindows="true"
    >



    <include
        layout="@layout/layer_header"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_value_dp50"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/dimen_value_dp10"
        android:layout_marginRight="@dimen/dimen_value_dp10"
        android:gravity="center_vertical"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/color_3d3d3d"
            android:textSize="@dimen/dimen_value_dp15"
            android:text="出库单号："
            />
        <TextView
            android:id="@+id/tv_number"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="@color/color_3d3d3d"
            />


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_value_dp50"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/dimen_value_dp10"
        android:layout_marginRight="@dimen/dimen_value_dp10"
        android:gravity="center_vertical"
        android:visibility="visible"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/color_3d3d3d"
            android:textSize="@dimen/dimen_value_dp15"
            android:text="仓库ID："
            />
        <TextView
            android:id="@+id/tv_ckid"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="@color/color_3d3d3d"
            />



    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_value_dp50"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/dimen_value_dp10"
        android:layout_marginRight="@dimen/dimen_value_dp10"
        android:gravity="center_vertical"
        android:visibility="visible"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/color_3d3d3d"
            android:textSize="@dimen/dimen_value_dp15"
            android:text="仓库名称："
            />
        <TextView
            android:id="@+id/tv_ckmc"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="@color/color_3d3d3d"
            />



    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_value_dp50"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/dimen_value_dp10"
        android:layout_marginRight="@dimen/dimen_value_dp10"
        android:gravity="center_vertical"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/color_3d3d3d"
            android:textSize="@dimen/dimen_value_dp15"
            android:text="库    位："
            />
        <TextView
            android:id="@+id/tv_kw_number"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="@color/color_3d3d3d"
            />

        

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_value_dp50"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/dimen_value_dp10"
        android:layout_marginRight="@dimen/dimen_value_dp10"
        android:gravity="center_vertical"
        android:visibility="visible"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/color_3d3d3d"
            android:textSize="@dimen/dimen_value_dp15"
            android:text="装箱单ID："
            />
        <TextView
            android:id="@+id/tv_zxdid"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textColor="@color/color_3d3d3d"
            />



    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/dimen_value_dp50"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/dimen_value_dp10"
        android:layout_marginRight="@dimen/dimen_value_dp10"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/color_3d3d3d"
            android:textSize="@dimen/dimen_value_dp15"
            android:text="备    注："
            />
        <TextView
            android:id="@+id/tv_bz"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/color_3d3d3d"
            android:text=""
            android:inputType="textMultiLine"
            android:minLines="1"
            android:lines="4"
            />


    </LinearLayout>





</LinearLayout>