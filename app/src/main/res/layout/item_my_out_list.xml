<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/ll_item"
            android:layout_marginTop="@dimen/dimen_value_dp8"
            android:orientation="vertical"
            android:layout_height="@dimen/dimen_value_dp140"
            android:layout_width="match_parent"
            android:background="@drawable/shap_main_hd">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_value_dp45"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp15"
                android:text="装箱单单号："
                android:layout_marginLeft="@dimen/dimen_value_dp10"
                />
            <TextView
                android:id="@+id/tv_userName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp15"
                android:text="X156431"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="@dimen/dimen_value_dp1"
                android:layout_weight="1"
                />

            <TextView
                android:id="@+id/tv_wancheng"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="已出库"
                android:textSize="@dimen/dimen_value_sp12"
                android:textColor="@color/color_2282EE"
                android:background="@drawable/shap_main_wwc1"
                android:paddingLeft="@dimen/dimen_value_dp10"
                android:paddingRight="@dimen/dimen_value_dp10"
                android:paddingBottom="@dimen/dimen_value_dp3"
                android:paddingTop="@dimen/dimen_value_dp3"
                android:layout_marginRight="@dimen/dimen_value_dp10"
                />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_value_dp1"
            android:layout_marginLeft="@dimen/dimen_value_dp10"
            android:layout_marginRight="@dimen/dimen_value_dp10"
            android:background="@color/color_bfbfbf"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/dimen_value_dp10"
                >
                <TextView
                    android:id="@+id/tv_chepai"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:text="净重：10kg"
                    android:textColor="@color/color_989898"
                    android:textSize="@dimen/dimen_value_sp12"
                    android:gravity="center_vertical"
                    />

                <TextView
                    android:id="@+id/tv_vin"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:text="毛重：8kg"
                    android:textColor="@color/color_989898"
                    android:textSize="@dimen/dimen_value_sp12"
                    android:gravity="center_vertical"
                    />

                <TextView
                    android:id="@+id/tv_xinghao"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:text="库位号：xxx"
                    android:textColor="@color/color_989898"
                    android:textSize="@dimen/dimen_value_sp12"
                    android:gravity="center_vertical"
                    />


                <TextView
                    android:id="@+id/tv_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:text="货物名称：木材"
                    android:textColor="@color/color_989898"
                    android:textSize="@dimen/dimen_value_sp12"
                    android:gravity="center_vertical"
                    />

            </LinearLayout>

            <View
                android:layout_width="0dp"
                android:layout_height="@dimen/dimen_value_dp1"
                android:layout_weight="1"
                />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_marginRight="@dimen/dimen_value_dp10"
                >
                <ImageView
                    android:layout_width="@dimen/dimen_value_dp15"
                    android:layout_height="@dimen/dimen_value_dp15"
                    android:src="@mipmap/right_jiantou"
                    />

            </LinearLayout>





        </LinearLayout>



    </LinearLayout>

</LinearLayout>