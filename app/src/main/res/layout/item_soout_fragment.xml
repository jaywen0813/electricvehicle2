<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_f5f5f5">

    <com.flyco.roundview.RoundLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:rv_cornerRadius="@dimen/dimen_value_dp5"
        android:layout_marginRight="@dimen/dimen_value_dp10"
        android:layout_marginLeft="@dimen/dimen_value_dp10"
        app:rv_backgroundColor="@color/color_50d2c2"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="visible">

            <TextView
                style="@style/textviewdetailleftstyle"
                android:text="工作单号" />


            <TextView
                android:id="@+id/tv_gzdh"
                style="@style/textviewdetailrightstyle" />


        </LinearLayout>

        <View style="@style/viewline_heng" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="visible">

            <TextView
                style="@style/textviewdetailleftstyle"
                android:text="日       期" />

            <TextView
                android:id="@+id/tv_date"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />


        </LinearLayout>


        <View style="@style/viewline_heng"
            />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="visible">

            <TextView
                style="@style/textviewdetailleftstyle"
                android:text="装箱单号" />

            <TextView
                android:id="@+id/tvNumber"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />


        </LinearLayout>

        <View style="@style/viewline_heng"

            />




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_value_dp50"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                style="@style/textviewdetailleftstyle"
                android:text="库       位" />

            <TextView
                android:id="@+id/tv_kw_number"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />


        </LinearLayout>

        <View style="@style/viewline_heng"
            android:visibility="gone"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="visible">


            <TextView
                android:id="@+id/tv_djx"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="第  箱"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />

            <View style="@style/viewline_shu" />

            <TextView
                android:id="@+id/tv_gjx"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="共  箱"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14"

                />

        </LinearLayout>

        <View style="@style/viewline_heng" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="visible">

            <TextView
                android:id="@+id/tv_chang"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left|center_vertical"
                android:text="长:"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />

            <View style="@style/viewline_shu" />

            <TextView
                android:id="@+id/tv_kuan"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left|center_vertical"
                android:text="宽:"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />

            <View style="@style/viewline_shu" />

            <TextView
                android:id="@+id/tv_gao"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left|center_vertical"
                android:text="高:"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />
        </LinearLayout>

        <View style="@style/viewline_heng" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="visible">

            <TextView
                android:id="@+id/tv_jingzhong"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left|center_vertical"
                android:text="净重:"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />

            <View style="@style/viewline_shu" />

            <TextView
                android:id="@+id/tv_maozhong"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left|center_vertical"
                android:text="毛重:"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />

            <View style="@style/viewline_shu" />

            <TextView
                android:id="@+id/tv_zhuangtai"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left|center_vertical"
                android:text="状态:"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />
        </LinearLayout>

        <View style="@style/viewline_heng" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="visible">

            <TextView
                android:id="@+id/tv_djgd"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left|center_vertical"
                android:text="单据归档:"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />

            <View style="@style/viewline_shu" />

            <TextView
                android:id="@+id/tv_djdy"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left|center_vertical"
                android:text="单据打印:"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />

            <View style="@style/viewline_shu" />

            <TextView
                android:id="@+id/tv_dycs"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left|center_vertical"
                android:text="打印次数:"
                android:textColor="@color/color_3d3d3d"
                android:textSize="@dimen/dimen_value_sp14" />
        </LinearLayout>



    </com.flyco.roundview.RoundLinearLayout>





</LinearLayout>